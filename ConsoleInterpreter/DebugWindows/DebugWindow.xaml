<Window x:Class="DebugGUI.DebugWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DebugGUI"
        mc:Ignorable="d"
        Title="DebugWindow" Height="800" Width="600">
    <Grid>
        <Grid.ColumnDefinitions>
        </Grid.ColumnDefinitions>

        <TabControl Margin="5">
            <TabItem Header=" Tokens ">
                <ListBox Name="lstTokens" Margin="5"></ListBox>
            </TabItem>
            <TabItem Header=" Tree ">
                <TreeView x:Name="tvTree" Margin="5" ></TreeView>
            </TabItem>
        </TabControl>
        
    </Grid>
</Window>
